<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:GetStartedApp.ViewModels"
        x:Class="GetStartedApp.Views.SecondView"
        x:DataType="vm:SecondViewModel"
        Width="1000" Height="600"
        Title="Second View"
        WindowStartupLocation="CenterScreen">
        
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <TextBlock Text="Successfully logged in." FontSize="20" HorizontalAlignment="Center"/>
        <Button Content="Go back" Command="{Binding GoBackCommand}" HorizontalAlignment="Center" Margin="0,10,0,0"/>
    </StackPanel>
</Window>
